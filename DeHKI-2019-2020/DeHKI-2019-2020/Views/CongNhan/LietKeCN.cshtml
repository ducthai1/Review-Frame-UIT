@{
}

<table border="1">
    <tr>
        <th>Mã công nhân</th>
        <th>Tên công nhân</th>
        <th>Chức năng</th>
    </tr>
    @foreach (var item in ViewBag.listCN)
    {
        <tr>
            <td>@item.MaCongNhan</td>
            <td>@item.TenCongNhan</td>
            <td>
                <a href="/CongNhan/DeleteCN?MaCongNhan=@item.MaCongNhan">Delete</a>
                <a href="/CongNhan/ViewCN?MaCongNhan=@item.MaCongNhan">View</a>
            </td>
        </tr>
    }
</table>
